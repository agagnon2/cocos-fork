    "highlight.js": "^11.5.0",
    "highlightjs": "^9.16.2"
  },
  "devDependencies": {
    "@stencil/core": "2.13.0",
    "gts": "3.1.0",
    "netlify-cli": "9.3.0",
    "prettier": "2.5.1",
    "rollup-plugin-dotenv": "0.3.0",
    "rollup-plugin-html": "0.2.1"
  }
}
